# RPI Version Environment Variables
# Lightweight monitoring with core features only

# ============================================================================
# VERSION CONTROL
# ============================================================================
VERSION_TYPE=RPI
ENABLE_AI_FEATURES=false

# ============================================================================
# APPLICATION
# ============================================================================
ENVIRONMENT=production
TZ=Asia/Seoul

# ============================================================================
# DATABASE
# ============================================================================
TS_DSN=postgresql://water_user:water_password@rpi-db:5432/water_db

# ============================================================================
# PORTS (RPI uses 13000-13001)
# ============================================================================
FRONTEND_PORT=13000
BACKEND_PORT=13001
